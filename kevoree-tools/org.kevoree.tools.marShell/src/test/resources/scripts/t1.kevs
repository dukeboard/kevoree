tblock{

   // include("")

    //ADD INSTANCE
    addNode nodeSalon,nodeSalon2
    addComponent light@nodeSalon : FakeLight
    addComponent switch@nodeSalon : FakeSwitch

    //CHANNEL
    addChannel hub1 : MyHubType
    addChannel hub2 : MyHubType


    addChannel hub3 : MyHubType
    removeChannel hub3 : MyHubType

    //BINDINGS
    bind light.on@nodeSalon => hub1
    bind switch.on@nodeSalon => hub1
    bind light.off@nodeSalon => hub2
    bind switch.off@nodeSalon => hub2

}